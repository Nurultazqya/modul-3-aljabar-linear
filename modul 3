import numpy as np

# no 1
A1 = np.array([[2, 3], [1, -1]])
b1 = np.array([7, 1])
hasil = np.linalg.solve(A1, b1)
print(f"x = {hasil[0]}, y= {hasil[0]}",hasil)
 
from sympy import symbols, Eq, solve

# no 2
x, y, z = symbols('x y z')
eq3 = Eq(x + 2*y + z, 10)
eq4 = Eq(3*x - y + 2*z, 5)
eq5 = Eq(-2*x + 3*y - z, -9)
hasil_2 = solve((eq3, eq4, eq5), (x, y, z))
print("hasil 2 :", hasil_2)
